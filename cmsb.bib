	issn = {1553-7358},
	journal = {PLOS Computational Biology},
	keywords = {Phosphorylation, Cell binding, Cell membranes, Dimerization, Dimers, Membrane receptor signaling, Protein kinase signaling cascade, SH2 domains},
	language = {en},
	month = apr,
	note = {Publisher: Public Library of Science},
	number = {4},
	pages = {e1000364},
	title = {A {Bipolar} {Clamp} {Mechanism} for {Activation} of {Jak}-{Family} {Protein} {Tyrosine} {Kinases}},
