	issn = {1751-8849},
	journal = {IET systems biology},
	month = sep,
	number = {5},
	pages = {342--351},
	pmcid = {PMC2628550},
	pmid = {19045829},
	title = {Domain-{Oriented} {Reduction} of {Rule}-{Based} {Network} {Models}},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC2628550/},
	urldate = {2023-02-10},
